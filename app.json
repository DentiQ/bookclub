{
  "expo": {
    "name": "Bookclub in Drawer ",
    "slug": "bookclub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fac8af"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.teamDrawer.bookclubInDrawer",
      "supportsTablet": false
    },
    "android": {
      "package": "com.teamDrawer.bookclubInDrawer",
      "permissions" : ["CAMERA"],
      "versionCode" : 1,
      "icon" : "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon_foreground.png",
        "backgroundColor": "#fac8af"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
